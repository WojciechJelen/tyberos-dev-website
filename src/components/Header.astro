---
import Navigation from "./Navigation.astro";

export interface NavbarItem {
  href: string;
  label: string;
}

const navbarItems = [
  {
    href: "/",
    label: "Home",
  },
  {
    href: "/about/",
    label: "About",
  },
  {
    href: "/blog/",
    label: "Blog",
  },
  {
    href: "/tags/",
    label: "Tags",
  },
];
---

<header>
  <Navigation navigationItems={navbarItems} />
</header>
